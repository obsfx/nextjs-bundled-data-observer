{
  "manifest_version": 3,
  "name": "NextJS 13+ Bundled Data Observer",
  "version": "0.0.1",
  "description": "A chrome extension to observe bundled data to client from a NextJS 13+ application.",
  "permissions": ["activeTab", "storage", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "icon-128.png",
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["*.js", "*.css"],
      "matches": ["*://*/*"]
    }
  ]
}
